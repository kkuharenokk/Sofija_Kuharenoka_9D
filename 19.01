#include <iostream>
using namespace std;
bool isNumber ( string s)
{
	for (int i = 0; i < s.length(); i++)
		if (isdigit(s[i]) == true);
			return true;
	return false;
}
int main() {
	string pass;
	do {

		cout << "Enter my password" << endl;
		cin >> pass;
		if (isNumber(pass)) { cout << "That is a letter! "; };

	} while (pass != "1234");
	return 0;


}

