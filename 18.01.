#include <iostream>
using namespace std;
int main() {
	int age = 14; 
	cout << "__________" << endl;
	cout << "login:" << endl;
		string login = "abc", xyz;
		cout << "password:" << endl;
		string password = "123", p1;
	cout << "__________" << endl;
	if (xyz == login) { cout << age; }
	else cout << "wrong input!";
	cin >> p1;
	if (p1 == password) { cout << age; }
	else cout << "wrong input!";
}
